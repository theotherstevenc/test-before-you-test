{
  "manifest_version":2,
  "name":"Email is Out There",
  "version":"1.0",
  "content_scripts":[
    {
      "matches":[
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions":["tabs"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
